package ar.edu.unlp.info.oo1.Ejercicio4_FigurasYCuerpos;

public class Cuadrado {
	
	private double lado;
	private double lado2;
	
	
	
	public double getlado() {
		return lado;
	}
	
	public void setLado(double lado) {
		this.lado = lado;
	}
	
	public double getPerimetro(double unLado) {
		double aux;
		aux = unLado * 4;
		return aux;
	}

	
	public double getArea(double unLado) {
		double aux;
		aux = unLado * unLado;
		return aux;
	}
	
	
	
	
	
	
	
	
}
